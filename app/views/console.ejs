<!doctype html>
<html lang="pt-br">

<head>
  <title>
    <%= title%>
  </title>
  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!-- Compiled and minified CSS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
  <style>
    pre{
    }
  </style>
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
  <header>
    <% include ./partials/header %>
  </header>
  <div class="row">

  <div class="col s6 offset-s3">
  
    <ul class="collapsible" data-collapsible="expandable">
    <%for(var i=0; i<quotes.length; i++){%>
      <li>
      <div class="collapsible-header"><i class="material-icons">filter_drama</i><span class="text-right"> <%= quotes[i].date.replace("UPC:","")%></span></div>
      <div class="collapsible-body">
      <label>Path</label>
      <pre class="grey lighten-3"> <%- JSON.stringify(quotes[i].path,undefined,2) %></pre>
      <label>Headers</label>
      <pre class="grey lighten-3"> <%- JSON.stringify(quotes[i].headers,undefined,2) %></pre>
      <label>Body</label>
      <pre class="grey lighten-3"> <%- JSON.stringify(quotes[i].body,undefined,2) %></pre></div>
    </li>
    <%};%>
  </ul>
  </div>      
</div>
  <!--Import jQuery before materialize.js-->
  <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <!-- Compiled and minified JavaScript -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
</body>
<script>
  $(document).ready(function(){
    $('.collapsible').collapsible();
  });
       
</script>
</html>